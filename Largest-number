//How to find largest number in C language
#include<stdio.h>
int main(int argc, char const *argv[])
{
    int a,b,c;
    printf("Enter 3 numbers:");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b){
        if(a>c){
            printf("Largest number: %d",a);
        }
        else{
            printf("Largest number: %d",c);
        }
    }
    else if(b>c){
        if(b>a){
            printf("Largest number: %d",b);
        }
        else{
            printf("Largest number: %d",c);
        }
    }
    else{
        printf("Largest number: %d",c);
    }
    return 0;
}
